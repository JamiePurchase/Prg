function artColourList(color)
{
	colorRGB(255,255,255)
	if(color=="BattleFuryActive"){colorRGB(225,90,0)}
	if(color=="BattleFuryStandard"){colorRGB(255,255,100)}
	*if(color=="OptionActive"){colorRGB(100,100,255)}
	*if(color=="OptionBorder"){colorRGB(150,150,255)}
	*if(color=="OptionStandard"){colorRGB(50,50,200)}
	if(color=="OptionActive"){colorRGB(100,255,100)}
	if(color=="OptionBorder"){colorRGB(150,255,150)}
	if(color=="OptionStandard"){colorRGB(50,200,50)}
	if(color=="Transparent"){colorRGB(0,255,0)}
}
